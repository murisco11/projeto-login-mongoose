<% if (errors && errors.length> 0) { %>
  <div class="flash error">
    <% errors.forEach(erro=> { %>
      <%= erro %><br>
        <% }) %>
  </div>
  <% } %>

    <% if (success && success.length> 0) { %>
      <div class="flash success">
        <% success.forEach(msg=> { %>
          <%= msg %><br>
            <% }) %>
      </div>
      <% } %>